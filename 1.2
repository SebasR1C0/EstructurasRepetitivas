#include <iostream>
#include <conio.h>
using namespace std;
void imp(int a) {
	int suma = 0, b = 1;
	while (b <= a) {
		suma = suma + b;
		b += 2;
	}
	cout << "La suma de los numero impares es: " << suma << endl;
}
int main() {
	int a;
	cout << "Ingrese el numero mayor " << endl;
	cin >> a;
	imp(a);
	return 0;
}
