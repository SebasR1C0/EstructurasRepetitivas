#include <iostream>
#include <cstdlib>
#include <conio.h>
using namespace std;
void ale(int n) {
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= i; j++) {
			cout << j << " ";
		}
		cout << endl;
	}
}
int main() {
	int n;
	double a;
	do {
		cout << "Ingrese un numero: ";
		cin >> n;
	} while (n > 10 || 1 > n);
	ale(n);
	return 0;
}
