#include <iostream>
#include <cstdlib>
#include <conio.h>
using namespace std;
void ale(int n) {
	int a;
	cout << "El numero invertido es: ";
	while (n>0) {
		a = n % 10;
		if (n == 0) {
			break;
		}
		cout << a;
		n /= 10;
	}	
}
int main() {
	int n;
	cout << "Ingrese un numero"<< endl;
	cin >> n;
	ale(n);
	return 0;
}
